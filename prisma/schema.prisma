generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}



model Email {
  id        Int @id @default(autoincrement())
  email_id  BigInt
  subject   String
  preview   String
  name      String
  user_id   String
}

model User {
  id                Int    @id @default(autoincrement())
  hub_id            BigInt @unique
  hub_domain        String 
}
